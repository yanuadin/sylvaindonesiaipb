<?php

namespace [namespace];

use Livewire\Component;
use Livewire\Attributes\On;

class [class] extends Component
{
    public function render()
    {
        return view('[view]');
    }

    /**
    * @description : default resetting all form
    */
    #[On('resetForm')]
    public function resetForm(): void
    {
        $this->reset();
    }
}
